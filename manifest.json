{
  "manifest_version": 3,
  "name": "Accept Cookies",
  "description": "Accepts cookie policies so you can get back to browsing. Icon designed by FreePik.",
  "version": "0.02",
  "content_scripts": [
    {
      "js":["content.js"],
      "matches":["<all_urls>"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "permissions": ["scripting","webNavigation"],
  "background": {
    "service_worker": "service.js"
  },
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
